{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "mongo": true
  },
  "rules": {
    "no-console":"warn",
    "no-unused-vars":"warn"
  }
}